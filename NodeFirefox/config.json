{
  "capabilities": [
    {
      "browserName": "*firefox",
      "maxInstances": 1,
      "seleniumProtocol": "Selenium",
      "browser-version": "${FIREFOX_VERSION}",
      "version": "${FIREFOX_VERSION}",
      "platform": "Linux",
      "javascriptEnabled":true
    },
    {
      "browserName": "firefox",
      "maxInstances": 1,
      "seleniumProtocol": "WebDriver",
      "browser-version": "${FIREFOX_VERSION}",
      "version": "${FIREFOX_VERSION}",
      "platform": "Linux",
      "javascriptEnabled":true
    }
  ],
  "configuration": {
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "maxSession": 1,
    "port": 5555,
    "register": true,
    "registerCycle": 5000
  }
}
